#include <iostream>
using namespace std;


void frequency(int arr[],int n){
    int freq=1;
    for(int i=0;i<n;i++){
        if(arr[i]==arr[i+1]){
            freq=freq+1;
        }
        else{
            cout<<"frequency of "<<arr[i]<<" is "<<freq;
            cout<<endl;
            freq=1;
        }
    }
}
int main(){
    int a[]={1,2,2,3,4,5,5,5,6};
    frequency(a,9);
    
}
