#include <iostream>

using namespace std;


void reverse(int arr[],int n){
    int i;
    int j;
    int temp[n];
    cout<<"The reversed array is"<<endl;
    for(int i=n-1,j=0;i>=0;i--,j++){
        temp[j]=arr[i];
        cout<<temp[j]<<" ";
    }
    
}
int main()
{
    int arr[]={1,2,3,4,5,6};
    int n=6;
    reverse(arr,n);
}
